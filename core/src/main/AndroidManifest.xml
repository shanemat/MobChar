<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="cz.cvut.fit.mobchar.core">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name=".presentation.ContentSelectionActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".presentation.about.AboutActivity"
                  android:parentActivityName=".presentation.ContentSelectionActivity"
                  android:label="@string/about_label"/>
    </application>
</manifest>
